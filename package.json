{
  "name": "vitrine-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/vitrine-site/stats.json",
    "build:fast": "ng build --configuration production --optimization=false",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss}\"",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run analyze:size",
    "analyze:size": "npx size-limit",
    "analyze:bundle": "npx webpack-bundle-analyzer dist/vitrine-site/stats.json",
    "serve:prod": "ng serve --configuration production",
    "serve:fast": "ng serve --configuration development --optimization=false",
    "e2e": "ng e2e",
    "pwa": "ng add @angular/pwa",
    "update": "ng update",
    "update:check": "ng update --dry-run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@angular/service-worker": "^17.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "size-limit": "^10.0.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "typescript": "~5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ]
}
